{
  "buildCommand": "cd nextjs-version && npm install && npm run build",
  "outputDirectory": "nextjs-version/.next",
  "devCommand": "cd nextjs-version && npm run dev",
  "installCommand": "cd nextjs-version && npm install",
  "framework": "nextjs",
  "rootDirectory": "nextjs-version",
  "env": {
    "DATABASE_URL": "@database_url",
    "NEXTAUTH_SECRET": "@nextauth_secret",
    "CPX_APP_ID": "@cpx_app_id",
    "CPX_SECURE_HASH": "@cpx_secure_hash"
  }
}